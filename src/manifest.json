{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "登录",
      "identifier": "hs.plugins.login",
      "script": "./login.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    },
    {
      "name": "切换工具栏",
      "identifier": "hs.plugins.changeTool",
      "script": "./changeTool.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Startup": "onStartup",
          "ArtboardChanged":"onArtboardChanged",
          "OpenDocument":"onOpenDocument",
          "CloseDocument":"onCloseDocument"
        }
      }
    },
    {
      "name": "刷新",
      "identifier": "hs.plugins.refresh",
      "script": "./refresh.js",
      "handlers": {
        "run": "onRun"
      }
    },
    {
      "name": "退出登录",
      "identifier": "hs.plugins.logout",
      "script": "./logout.js",
      "handlers": {
        "run": "onRun"
      }
    }
  ],
  "menu": {
    "title": "hs-plugins",
    "items": [
      "hs.plugins.login",
      "hs.plugins.changeTool",
      "hs.plugins.refresh",
      "hs.plugins.logout"
    ]
  }
}